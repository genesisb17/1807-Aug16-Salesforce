<apex:page controller="CustomAccountListControl">
	<apex:form >
		<apex:pageBlock >
			<apex:pageBlockTable value="{!accountList}" var="account">
				<apex:column value="{!account.name}" />
				<apex:column value="{!account.phone}" />
				<apex:column value="{!account.fax}" />
				<apex:column value="{!account.website}" />
			</apex:pageBlockTable>
		</apex:pageBlock>
	</apex:form>
</apex:page>